{
    "name": "stardothosting/brandenburg",
    "description": "Laravel Authentication Package",
    "authors": [
        {
            "name": "Shift8 Web",
            "email": "sales@shift8web.com"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Silvanite\\Brandenburg\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Silvanite\\Brandenburg\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Silvanite\\Brandenburg\\Providers\\BrandenburgServiceProvider"
            ],
            "aliases": {
                "BrandenburgPolicy": "Silvanite\\Brandenburg\\Facades\\PolicyFacade"
            }
        }
    },
    "require": {
        "php": ">=7.3",
        "laravel/framework": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0",
        "squizlabs/php_codesniffer": "^3.5",
        "mockery/mockery": "^1.3.1"
    }
}
